@model ETForum.Models.StudySession

<form asp-action="Create">
	<div class="form-group">
		<label asp-for="pocetak"></label>
		<input asp-for="pocetak" class="form-control" type="datetime-local" />
		<span asp-validation-for="pocetak" class="text-danger"></span>
	</div>
	<div class="form-group">
		<label asp-for="kraj"></label>
		<input asp-for="kraj" class="form-control" type="datetime-local" />
		<span asp-validation-for="kraj" class="text-danger"></span>
	</div>
	<button type="submit" class="btn btn-primary">Kreiraj</button>
</form>